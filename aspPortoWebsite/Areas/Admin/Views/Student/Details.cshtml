@model Student
@{
    ViewData["Title"] = "Index";
}

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Fullname</th>
            <th>City</th>
            <th>Adress</th>
            <th>Group</th>

        </tr>
    </thead>
    <tbody>

            <tr>
                <th scope="row">@Model.Id</th>
                <th scope="row">@Model.Fullname</th>
                <td>@Model.Adress.City</td>
                <td>@Model.Adress.Address</td>
               <th>@Model.Group.Fullname</th>
            </tr>

    </tbody>
</table>
