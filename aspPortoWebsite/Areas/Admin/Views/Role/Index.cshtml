@model IEnumerable<IdentityRole>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>id</th>
            <th>name</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>
                    <a href="/Admin/Role/Update/@item.Id" style="color:red"><i class="fas fa-edit"></i></a>

                </td>
            </tr>
        }
    </tbody>
</table>